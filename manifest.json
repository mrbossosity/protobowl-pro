{
  "name": "Protobowl Pro",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://protobowl.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Protobowl Pro"
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}